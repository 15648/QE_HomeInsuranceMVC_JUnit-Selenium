<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>FilterServlet</filter-name>
    <filter-class>casestudy.homeinsurance.servlet.FilterServlet</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>FilterServlet</filter-name>
    <url-pattern>/BuyPolicy</url-pattern>
    <url-pattern>/GetQuote</url-pattern>
    <url-pattern>/Logout</url-pattern>
    <url-pattern>/PreCheckout</url-pattern>
    <url-pattern>/RetrievePolicy</url-pattern>
    <url-pattern>/RetrieveQuote</url-pattern>
    <url-pattern>/SetHomeowner</url-pattern>
    <url-pattern>/SetProperty</url-pattern>
    <url-pattern>/getquote.jsp</url-pattern>
    <url-pattern>/getstarted.jsp</url-pattern>
  </filter-mapping>
  <servlet>
    <display-name>admin</display-name>
    <servlet-name>admin</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>admin</servlet-name>
    <url-pattern>/admin/*</url-pattern>
  </servlet-mapping>
</web-app>